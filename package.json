{
  "name": "@parsimap/react-mapbox-gl",
  "version": "1.2.6",
  "description": "A Library to render mapboxgl mapview with power of parsimap services.",
  "author": "muhamadzolfaghari",
  "license": "MIT",
  "keywords": [
    "mapview",
    "mapbox-gl",
    "tile-server",
    "react",
    "map",
    "gis"
  ],
  "repository": {
    "url": "git://github.com/parsimap/react-mapbox-gl.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.ts",
  "engines": {
    "node": ">=10"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "mapbox-gl": "^1.12.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "scripts": {
    "build": "microbundle-crl --compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^25.1.4",
    "@types/mapbox-gl": "^2.7.4",
    "@types/node": "^12.12.38",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "cross-env": "^7.0.2",
    "gh-pages": "^2.2.0",
    "mapbox-gl": "^1.13.2",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "typescript": "^4.7.4"
  },
  "files": [
    "dist"
  ]
}
